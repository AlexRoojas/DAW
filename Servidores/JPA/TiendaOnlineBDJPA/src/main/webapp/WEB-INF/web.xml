        <servlet-class>com.daw.tiendaonlinebdjpa.logica.opCesta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cargaCateProv</servlet-name>
        <servlet-class>com.daw.tiendaonlinebdjpa.logica.cargaCateProv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>altaArt</servlet-name>
        <servlet-class>com.daw.tiendaonlinebdjpa.logica.altaArt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>generaFactura</servlet-name>
        <servlet-class>com.daw.tiendaonlinebdjpa.logica.generaFactura</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cargaArt</servlet-name>
        <url-pattern>/cargaArt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>operaciones</servlet-name>
        <url-pattern>/operaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compruebaUsu</servlet-name>
        <url-pattern>/compruebaUsu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>finalizar</servlet-name>
        <url-pattern>/finalizar</url-pattern>
    </servlet-mapping>

        <servlet-name>cargaCesta</servlet-name>
        <url-pattern>/cargaCesta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>opCesta</servlet-name>
        <url-pattern>/opCesta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cargaCateProv</servlet-name>
        <url-pattern>/cargaCateProv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>altaArt</servlet-name>
        <url-pattern>/altaArt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>generaFactura</servlet-name>
        <url-pattern>/generaFactura</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>

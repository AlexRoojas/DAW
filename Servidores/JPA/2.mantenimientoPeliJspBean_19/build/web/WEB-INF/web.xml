<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>operaciones</servlet-name>
        <servlet-class>operaciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consulta</servlet-name>
        <servlet-class>consulta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>puebaConex</servlet-name>
        <servlet-class>puebaConex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pruebaConex</servlet-name>
        <servlet-class>puebaConex</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>operaciones</servlet-name>
        <url-pattern>/operaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consulta</servlet-name>
        <url-pattern>/consulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>puebaConex</servlet-name>
        <url-pattern>/puebaConex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pruebaConex</servlet-name>
        <url-pattern>/pruebaConex</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>

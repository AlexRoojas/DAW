<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">  	
	
	
	<welcome-file-list>
		<welcome-file>login</welcome-file>
	</welcome-file-list>
	
	
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>obtenerCod</servlet-name>
		<servlet-class>obtenerCod</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>obtenerCod</servlet-name>
		<url-pattern>/obtenerCod</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>comprueba</servlet-name>
		<servlet-class>comprueba</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>comprueba</servlet-name>
		<url-pattern>/comprueba</url-pattern>
	</servlet-mapping>
	
	<context-param>
		<param-name>codigos</param-name>
		<param-value>aa1:usado;aa2:libre;aa3:alex;aa4:libre</param-value>
	</context-param>
	
	<context-param>
		<param-name>combiGana</param-name>
		<param-value>8;4;5</param-value>
	</context-param>
	

	
</web-app>

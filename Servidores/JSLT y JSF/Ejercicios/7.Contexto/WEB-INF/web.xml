<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">  	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>contexto</servlet-name>
		<servlet-class>com.entrada.contexto</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>contexto</servlet-name>
		<url-pattern>/contexto</url-pattern>
	</servlet-mapping>
	
	<context-param>
		<param-name>imgInv</param-name>
		<param-value>imagenes/invierno.jpg</param-value>
	</context-param>
	<context-param>
		<param-name>imgPrim</param-name>
		<param-value>imagenes/primavera.jpg</param-value>
	</context-param>
	<context-param>
		<param-name>imgVer</param-name>
		<param-value>imagenes/verano.jpg</param-value>
	</context-param>
	<context-param>
		<param-name>imgOto</param-name>
		<param-value>imagenes/otono.jpg</param-value>
	</context-param>
	
	
</web-app>

<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">  	
	
	
	<welcome-file-list>
		<welcome-file>index</welcome-file>
	</welcome-file-list>
	
	
	
	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>index</servlet-class>
		<init-param>
			<param-name>Parametros</param-name>
			<param-value>camiseta:10:200;gorra:14:300;zapatos:20:500;rinonera:3:1000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addArticulo</servlet-name>
		<servlet-class>addArticulo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addArticulo</servlet-name>
		<url-pattern>/addArticulo</url-pattern>
	</servlet-mapping>
	
	
	
	
	

	
</web-app>

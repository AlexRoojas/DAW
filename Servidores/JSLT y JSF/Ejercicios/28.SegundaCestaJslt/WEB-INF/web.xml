<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">  	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	

	<servlet>
		<servlet-name>modificaExistencias</servlet-name>
		<servlet-class>modificaExistencias</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>modificaExistencias</servlet-name>
		<url-pattern>/modificaExistencias</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Fin</servlet-name>
		<servlet-class>Fin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Fin</servlet-name>
		<url-pattern>/Fin</url-pattern>
	</servlet-mapping>


	
	<context-param>
		<param-name>Parametros</param-name>
		<param-value>Camiseta:10:7;Sudadera:15:12;Calcetines:10:5;Duende:5:25</param-value>
	</context-param>
	
</web-app>
